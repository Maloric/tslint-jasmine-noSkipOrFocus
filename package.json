{
  "name": "tslint-jasmine-noSkipOrFocus",
  "version": "1.0.0",
  "description": "Linting rules to disallow focused or skipped tests.",
  "scripts": {
    "lint": "tslint -c tslint.json src/**/*.ts test/**/*.ts",
    "release": "npm run build && rm -rf ./dist && tsc -p tsconfig.json && npm run copy:common && npm run prepare:package",
    "build": "rm -rf ./dist && tsc && npm run lint && npm t",
    "copy:common": "cp README.md dist",
    "prepare:package": "cat package.json | ts-node build/package.ts > dist/package.json",
    "test": "rm -rf ./dist && tsc && mocha dist/test/*.js dist/test/**/*.js",
    "test:watch": "rm -rf ./dist && tsc && mocha dist/test/*.js dist/test/**/*.js --watch"
  },
  "contributors": [
    "Jamie Morris <jamie@maloric.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maloric/tslint-jasmine-noSkipOrFocus.git"
  },
  "keywords": [
    "lint",
    "tslint",
    "jasmine"
  ],
  "author": {
    "name": "Jamie Morris",
    "email": "jamie@maloric.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maloric/tslint-jasmine-noSkipOrFocus/issues"
  },
  "homepage": "https://github.com/maloric/tslint-jasmine-noSkipOrFocus#readme",
  "devDependencies": {
    "@types/chai": "^3.4.33",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.41",
    "chai": "^3.5.0",
    "mocha": "3.0.2",
    "tslint": "3.14.0",
    "typescript": "^2.0.3"
  },
  "peerDependencies": {
    "tslint": "^3.9.0"
  }
}
